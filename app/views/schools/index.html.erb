<h1>Listing schools in <%= @district.jurisdiction_name %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schools.each do |school| %>
      <tr>
        <td><%= school.name%></td>
        <td><%= link_to 'Show', district_school_path(@district.id,school.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
