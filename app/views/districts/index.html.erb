<h1>Listing districts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.jurisdiction_name%></td>
        <td><%= link_to 'Show', district %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
